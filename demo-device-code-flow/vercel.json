{
  "version": 2,
  "name": "okta-device-code-flow-demo",
  "rewrites": [
    {
      "source": "/api/okta/(.*)",
      "destination": "/api/proxy"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/proxy.js": {
      "maxDuration": 10
    }
  }
} 